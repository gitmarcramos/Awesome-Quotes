{
  "name": "awesome-quotes",
  "description": "Awesome Quotes is a mobile first web app where you can browse or share your best punchlines and quotes!",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www -e .hbs,.js",
    "seed:user": "node ./bin/seed/Users.seed.js",
    "seed:quote": "node ./bin/seed/Quotes.seed.js",
    "seed:all": "npm run seed:user && npm run seed:quote"
  },
  "author": "Marc Ramos",
  "contributors": [
    "Paul Riveron",
    "Joey Jang"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/gitmarcramos/Awesome-Quotes"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.27.1",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "mongoose": "^6.0.12",
    "morgan": "~1.9.1",
    "multer": "^1.4.3",
    "multer-storage-cloudinary": "^4.0.0",
    "timeago.js": "^4.0.2"
  }
}
